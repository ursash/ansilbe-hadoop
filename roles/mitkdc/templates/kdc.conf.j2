[kdcdefaults]
  kdc_ports = 88
  kdc_tcp_ports = 88

[realms]
  {{ default_realm|upper }} = {
    database_name = /var/kerberos/krb5kdc/principal
    max_renewable_life = 7d
    master_key_type = aes256-cts
    acl_file = /var/kerberos/krb5kdc/kadm5.acl
    dict_file = /usr/share/dict/words
    admin_keytab = /var/kerberos/krb5kdc/kadm5.keytab
    supported_enctypes = aes256-cts-hmac-sha1-96:normal aes128-cts-hmac-sha1-96:normal des3-cbc-sha1:normal des-cbc-md5:normal arcfour-hmac-md5:normal
  }
